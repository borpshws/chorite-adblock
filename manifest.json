{
    "manifest_version": 2,
    "name": "Chorite Adblock",
    "description": "Block T's political ads",
    "version": "1.0",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    }
}